<!DOCTYPE html>
<html lang="en">

<head>
    <%- include ("../partials/header.ejs") %>
</head>

<body>
    <%- include ("../partials/nav.ejs") %>

        <h1>System Info</h1>
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Key</th>
                    <th scope="col">Value</th>
                </tr>
            </thead>


            <tbody>
                <% for( const [key, value] of Object.entries(systemInfo) ) { %>
                    <tr>
                        <th scope="row">
                            <%= key %>
                        </th>
                        <td>
                            <%= value %>
                        </td>
                    </tr>
                    <% } %>
            </tbody>

        </table>


</body>

</html>